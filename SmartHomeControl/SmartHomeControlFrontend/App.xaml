<Application x:Class="SmartHomeControlFrontend.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="EncodeSans">pack://application:,,,/Resources/FontFamily/#Encode Sans</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/ResourceLibrary/Style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
